<h1>Listing notebooks</h1>

<table>
  <tr>
    <th>Researcher</th>
  </tr>

<% @notebooks.each do |notebook| %>
  <tr>
    <td><%=h notebook.researcher %></td>
    <td><%= link_to 'Show', notebook %></td>
    <td><%= link_to 'Edit', edit_notebook_path(notebook) %></td>
    <td><%= link_to 'Destroy', notebook, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New notebook', new_notebook_path %>